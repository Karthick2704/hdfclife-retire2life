<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <sectionOneDetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Social Share Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <left-icon
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="left Icon Image"
                                                fileReferenceParameter="./leftIcon"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./leftIconName"/>
                                            <leftIconAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Left Icon Alt Text"
                                                name="./leftIconAltText"/>
                                            <right-icon
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Right Icon"
                                                fileReferenceParameter="./rightIcon"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./rightIconName"/>
                                            <rightIconAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Right Icon Alt Text"
                                                name="./rightIconAltText"/>
                                            <sectionOneBrandDetails
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Social Share  Details"
                                                name="./sectionOneBrandDetails"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    granite:class="cmp-form-options__editor-item-multifield-composite-item coral-Well"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./sectionOneBrandDetails">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <targetName
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Target Name"
                                                            name="./targetName"/>
                                                        <title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Title"
                                                            name="./title"/>
                                                    </items>
                                                </field>
                                            </sectionOneBrandDetails>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </sectionOneDetails>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
