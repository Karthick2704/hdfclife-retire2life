                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Configure the subContentTwo"
                                                            fieldLabel="Designation"
                                                            name="./subContentTwo"/>
                                                        <subContentThree
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Configure the subContentThree"
                                                            fieldLabel="Company"
                                                            name="./subContentThree"/>
                                                    </items>
                                                </field>
                                            </sectionOneBrandDetails>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </testimonailTabs>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
