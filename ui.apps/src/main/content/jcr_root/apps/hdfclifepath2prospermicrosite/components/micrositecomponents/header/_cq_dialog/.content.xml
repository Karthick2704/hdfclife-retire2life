<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <headerDetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Header Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <logoImage
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Logo Image"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./logoImage"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <logoImageAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Logo Image Alt Text"
                                                name="./logoImageAltText"/>
                                            <logoImageLink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Logo Image Link"
                                                name="./logoImageLink"
                                                required="{Boolean}true"
                                                rootPath="/content/hdfclifepath2prosper"/>
                                            <mobileSecondaryLogoImageImage
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Mobile Secondary Logo Image"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./mobileSecondaryLogoImage"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <mobileSecondaryLogoImageAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Secondary Logo Image Alt Text"
                                                name="./mobileSecondaryLogoImageAltText"/>
                                            <mobileSecondaryLogoImageLink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Mobile Secondary Logo Image Link"
                                                name="./mobileSecondaryLogoImageLink"
                                                required="{Boolean}true"
                                                rootPath="/content/hdfclifepath2prosper"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Description"
                                                name="./description"
                                                showBreadcrumb="{Boolean}true"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins jcr:primaryType="nt:unstructured">
                                                    <format
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <justify
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <lists
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <paraformat
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*">
                                                        <formats jcr:primaryType="nt:unstructured">
                                                            <default_p
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Paragraph"
                                                                tag="p"/>
                                                            <default_h1
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 1"
                                                                tag="h1"/>
                                                            <default_h2
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 2"
                                                                tag="h2"/>
                                                            <default_h3
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 3"
                                                                tag="h3"/>
                                                            <default_h4
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 4"
                                                                tag="h4"/>
                                                            <default_h5
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 5"
                                                                tag="h5"/>
                                                            <default_h6
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Heading 6"
                                                                tag="h6"/>
                                                            <default_blockquote
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Quote"
                                                                tag="blockquote"/>
                                                            <default_pre
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Preformatted"
                                                                tag="pre"/>
                                                            <notextformat
                                                                jcr:primaryType="nt:unstructured"
                                                                description="No - Text Format"
                                                                tag="notextformat"/>
                                                            <div
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Div format"
                                                                tag="div"/>
                                                            <hr
                                                                jcr:primaryType="nt:unstructured"
                                                                description="Horizontal Ruler"
                                                                tag="hr"/>
                                                        </formats>
                                                    </paraformat>
                                                    <links
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <image
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <findreplace
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <subsuperscript
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <spellcheck
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <misctools
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*">
                                                        <specialCharsConfig jcr:primaryType="nt:unstructured">
                                                            <chars jcr:primaryType="nt:unstructured">
                                                                <copyright
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#169;"/>
                                                                <trademark
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#8482;"/>
                                                                <registered
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#174;"/>
                                                                <emDash
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#8212;"/>
                                                                <pound
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#163;"/>
                                                                <nbsp
                                                                    jcr:primaryType="nt:unstructured"
                                                                    entity="&amp;#160;"/>
                                                            </chars>
                                                        </specialCharsConfig>
                                                    </misctools>
                                                    <styles
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*">
                                                        <styles jcr:primaryType="nt:unstructured">
                                                            <plainSpan
                                                                jcr:primaryType="nt:unstructured"
                                                                cssName="defaultSpan"
                                                                text="Default (add span tag)"/>
                                                            <lead
                                                                jcr:primaryType="nt:unstructured"
                                                                cssName="lead"
                                                                text="Lead"/>
                                                        </styles>
                                                    </styles>
                                                    <table
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <colorformat
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <styleformat
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                </rtePlugins>
                                                <uiSettings jcr:primaryType="nt:unstructured">
                                                    <cui jcr:primaryType="nt:unstructured">
                                                        <inline
                                                            jcr:primaryType="nt:unstructured"
                                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,image#imageProps,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table,colorformat#colorPicker,styleformat#styles]">
                                                            <popovers jcr:primaryType="nt:unstructured">
                                                                <justify
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                    ref="justify"/>
                                                                <lists
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                                    ref="lists"/>
                                                                <paraformat
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                                    ref="paraformat"/>
                                                                <styles
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="styles:getStyles:styles-pulldown"
                                                                    ref="styles"
                                                                    title="MyStyle"/>
                                                                <ACTStylesCommand
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="styles:getStyles:styles-pulldown"
                                                                    ref="ACTStylesCommand"/>
                                                            </popovers>
                                                        </inline>
                                                    </cui>
                                                </uiSettings>
                                            </description>
                                            <messageIcon
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Message Icon"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./messageIcon"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <messageIconTwo
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Message Icon Two"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./messageIconTwo"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <messageIconAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Message Icon Alt Text"
                                                name="./messageIconAltText"/>
                                            <mobileMessageIcon
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Mobile Message Icon"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./mobileMessageIcon"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <mobileMessageIconTwo
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Mobile Message Icon Two"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./mobileMessageIconTwo"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <mobileMessageIconAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Message Icon Alt Text"
                                                name="./mobileMessageIconAltText"/>
                                            <homePath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Home Path"
                                                name="./homePath"
                                                required="{Boolean}true"
                                                rootPath="/content/hdfclifepath2prospermicrosite"/>
                                            <menuIcon
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Home Icon"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./menuIcon"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <menuIconAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Home Icon Alt Text"
                                                name="./menuIconAltText"/>
                                            <mobileMenuIcon
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Mobile Menu Icon"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./mobileMenuIcon"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <mobileMenuIconAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Menu Icon Alt Text"
                                                name="mobileMenuIconAltText"/>
                                            <searchIcon
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Search Icon"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./searchIcon"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <searchIconAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Search Icon Alt Text"
                                                name="./searchIconAltText"/>
                                            <cancelIcon
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Cancel Icon"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./cancelIcon"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <cancelIconAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Cancel Icon Alt Text"
                                                name="./cancelIconAltText"/>
                                            <inputSearchIcon
                                                granite:class="cmp-image__editor-file-upload"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                class="cq-droptarget"
                                                fieldLabel="Input Search Icon"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./inputSearchIcon"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./listImage"/>
                                            <inputSearchIconAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Input Search Icon Alt Text"
                                                name="./searchIconAltText"/>
                                            <menuDisclaimer
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Menu Disclaimer"
                                                name="./menuDisclaimer"/>
                                            <searchDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Search Description"
                                                name="./searchDescription"/>
                                            <searchPlaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Search Placeholder Text"
                                                name="./searchPlaceholder"/>
                                            <searchPath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                emptyText="Select a search page"
                                                fieldLabel="Search Path"
                                                name="./searchPath"
                                                required="{Boolean}true"
                                                rootPath="/content/hdfclifepath2prosper"/>
                                            <mailSubject
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mail Subject"
                                                name="./mailSubject"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </headerDetails>
                    <navigationDetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Navigation Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sectionOneBrandDetails
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Navigation  Details"
                                                name="./sectionOneBrandDetails"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    granite:class="cmp-form-options__editor-item-multifield-composite-item coral-Well"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./sectionOneBrandDetails">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Title"
                                                            name="./title"/>
                                                        <targetPath
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            emptyText="Select a target page"
                                                            fieldLabel="Target Path"
                                                            name="./targetPath"
                                                            required="{Boolean}true"
                                                            rootPath="/content/hdfclifepath2prosper"/>
                                                    </items>
                                                </field>
                                            </sectionOneBrandDetails>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </navigationDetails>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
